<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC         "-//Hibernate/Hibernate Mapping DTD 3.0//EN"         "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.csuisse.LogEvent" table="LogEvent">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="state" column="state" />
		<property name="type" column="type" />
		<property name="host" column="host" />
		<property name="startTimestamp" column="starttimestamp" />
		<property name="endTimestamp" column="endtimestamp" />
		<property name="alert" column="alert" />
	</class>

</hibernate-mapping>
